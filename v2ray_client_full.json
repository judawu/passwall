            "streamSettings": {
                "network": "tcp",
                "security": "tls",
                "tlsSettings": {
                    "serverName": "@@@@-server-@@@@" // 换成你的域名
                }
            }
        },

    {
   // Shadowsocks
      "tag": "proxy10",

      "protocol": "shadowsocks",
      "settings": {
     "servers": [
       {
           "address": "@@@@-server-@@@@", // Shadowsocks 的服务器地址
            "method": "aes-128-gcm", // Shadowsocks 的加密方式
            "AEAD": true, // 是否开启 OTA，true 为开启
            "password": "@@@@@-Passwd-@@@@@", // Shadowsocks 的密码
            "port": 10005
       }
                   ]
                  }
    },
{
